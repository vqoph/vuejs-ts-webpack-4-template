{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "strict": true,
        "module": "es2015",
        "moduleResolution": "node",
        "target": "es6",
        "experimentalDecorators": true,
        "strictPropertyInitialization": false,
        "noImplicitAny": false,
        "baseUrl": ".",
        "paths": {
            "~/*": ["src/*"]
        }
    },
    "include": [
        "./src/**/*"
    ]
}
